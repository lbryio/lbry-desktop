    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "prettier": "^1.4.2",
    "rollup": "^1.8.0",
    "rollup-plugin-alias": "^2.0.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-flow": "^1.1.1",
